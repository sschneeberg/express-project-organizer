<h1>
    <%= project.name %>
</h1>

<a class="btn btn-primary" href="<%= project.githubLink %>" target="_blank">
    <i class="fa fa-github" aria-hidden="true"></i> Github Link
</a>

<a class="btn btn-primary" href="<%= project.deployLink %>" target="_blank">
    <i class="fa fa-globe" aria-hidden="true"></i> Deployed Link
</a>

<p>
    <%= project.description %>
</p>
<p>
    <% project.categories.forEach(category => { %>
        <span class="label label-primary"><%= category.dataValues.name %></span>
        <% }); %>
</p>
</br>
<a class="btn btn-primary" href="/projects/<%=project.id%>/edit">Edit</a>

<hr />

<a href="/">&larr; Home</a>
<a href="/categories"> | Categories &rarr;</a>